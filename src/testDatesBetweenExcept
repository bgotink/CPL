var utils = require('./utils');
var santa = new Date("December 6, 2012 11:13:00");
var christmas = new Date("December 25, 2012 22:13:00");
var newyear = new Date("January 1, 2013 00:00:01");
var sun = 1, mon = 0, tue = 1, wed = 0, thu = 1, fri = 1, sat = 0;
var pattern = sun*1 + mon*2 + tue*4 + wed*8 + thu*16 + fri*32 + sat*64;
utils.DatesBetweenExcept(santa, newyear, pattern, [christmas]);
